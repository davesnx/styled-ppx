/* Pointer Events Level 1 */
[%cx2 {|touch-action: auto|}];
[%cx2 {|touch-action: none|}];
[%cx2 {|touch-action: pan-x|}];
[%cx2 {|touch-action: pan-y|}];
[%cx2 {|touch-action: pan-x pan-y|}];
[%cx2 {|touch-action: manipulation|}];

/* Pointer Events Level 3 */
[%cx2 {|touch-action: pan-left|}];
[%cx2 {|touch-action: pan-right|}];
[%cx2 {|touch-action: pan-up|}];
[%cx2 {|touch-action: pan-down|}];
[%cx2 {|touch-action: pan-left pan-up|}];

/* Compatibility */
[%cx2 {|touch-action: pinch-zoom|}];
[%cx2 {|touch-action: pan-x pinch-zoom|}];
[%cx2 {|touch-action: pan-y pinch-zoom|}];
[%cx2 {|touch-action: pan-x pan-y pinch-zoom|}];
