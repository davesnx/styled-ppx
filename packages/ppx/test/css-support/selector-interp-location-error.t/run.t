      for_package: None,
      debug: false,
      use_threads: false,
      use_vmthreads: false,
      recursive_types: false,
      principal: false,
      transparent_modules: false,
      unboxed_types: false,
      unsafe_string: false,
      cookies: [],
    }
  ];
  let _className =
    CssJs.style([|
      CssJs.label("_className"),
      CssJs.selector(
        {js|.lolaso :nth-child(2n)|js},
        [|CssJs.color(CssJs.red)|],
      ),
    |]);
  let _className = [%ocaml.error "Parse error while reading token '2'"];
