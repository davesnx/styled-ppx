(copy_files
 (files ../js/**[!.pp].ml))

(rule
 (alias all)
 (action
  (progn
   (with-stdout-to
    bsconfig.json
    (run
     echo
     "{\"name\": \"@davesnx/styled-ppx/emotion\", \"sources\": [\".\"], \"bs-dependencies\": [\"@davesnx/styled-ppx/css\"]}"))
   (with-stdout-to
    package.json
    (run
     echo
     "{\"name\": \"@davesnx/styled-ppx/emotion\", \"dependencies\": { \"@emotion/css\": \"^11.11.2\" } }")))))
