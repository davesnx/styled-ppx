(copy_files
 (files ../js/**[!.pp].ml))

(rule
 (alias all)
 (action
  (progn
   (with-stdout-to
    bsconfig.json
    (run
     echo
     "{\"name\": \"bs-css-emotion\", \"sources\": [\".\"], \"bs-dependencies\": [\"bs-css\"]}"))
   (with-stdout-to
    package.json
    (run
     echo
     "{\"name\": \"bs-css-emotion\", \"dependencies\": { \"@emotion/css\": \"^11.11.2\" } }")))))
