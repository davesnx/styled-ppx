(test
 (name Tokenizer_test)
 (libraries
  styled-ppx.reason-css-lexer
  alcotest
  ppxlib
  ppxlib.astlib
  sedlex
  ppx_deriving.runtime))

; This creates an alias "css_lexer_test" that can be run with `dune build @css_lexer_test`

(rule
 (alias css_lexer_test)
 (deps Tokenizer_test.exe)
 (action
  (run %{deps})))
