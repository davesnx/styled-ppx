{
  "name": "styled-ppx-demo-rescript-v10-jsx4",
  "jsx": { "version": 4, "mode": "classic" },
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "sources": [{
    "dir": "src",
    "subdirs": false,
    "type": "dev"
  }, {
    "dir": "demo",
    "subdirs": false,
    "type": "dev"
  }],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "ppx-flags": [
    "../../_build/default/bin/bin.exe"
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": [
    "@rescript/react",
    "bs-css",
    "bs-css-emotion"
  ]
}
