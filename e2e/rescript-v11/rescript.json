{
  "name": "styled-ppx-demo-rescript-v11",
  "jsx": { "version": 4, "mode": "classic" },
  "bsc-flags": ["-bs-super-errors"],
  "warnings": {
    "number": "-48"
  },
  "sources": [
    {
      "dir": "src",
      "type": "dev"
    },
    {
      "dir": "../../_build/default/packages/css/rescript"
    },
    {
      "dir": "../../_build/default/packages/emotion/rescript"
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "ppx-flags": ["../../_build/default/packages/bin/bin.exe"],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": ["@rescript/react", "rescript-webapi"]
}
