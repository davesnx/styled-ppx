{
  "scopeName": "styled",
  "fileTypes": [
    "re",
    "ml"
  ],
  "injectionSelector": "L:source -comment -string",
  "patterns": [
    {
      "contentName": "meta.embedded.block.css",
      "begin": "(\\[%(styled\\.([a-zA-Z]*)))s*$",
      "end": "(?<=])",
      "patterns": [
        {
          "include": "source.css.styled"
        }
      ]
    }
  ]
}
